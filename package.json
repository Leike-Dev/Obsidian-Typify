{
    "name": "typify",
    "version": "1.1.0",
    "description": "Create attractive and customizable status indicators with Lucide icons for your metadata properties.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node dev/version-bump.mjs && git add manifest.json versions.json package-lock.json README.md docs/README_pt.md docs/README_es.md docs/README_fr.md docs/README_zh-CN.md",
        "gen-icons": "node dev/generate-icons.mjs",
        "commit": "node dev/commit-helper.mjs",
        "lint": "eslint ."
    },
    "keywords": [],
    "author": "Flavio Azurita",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.30.0",
        "@eslint/json": "0.14.0",
        "@types/node": "^22.0.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "eslint": "^9.30.0",
        "eslint-plugin-obsidianmd": "^0.1.9",
        "lucide-static": "^0.446.0",
        "obsidian": "latest",
        "tslib": "^2.8.0",
        "typescript": "~5.7.0",
        "typescript-eslint": "^8.35.1"
    }
}